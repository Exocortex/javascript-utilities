{
  "testRegex": "[\\w+]\\.test\\.(tsx?|js)$",
  "rootDir": "../../",
  "roots": [
    "<rootDir>/src",
    "<rootDir>/tests"
  ],
  "verbose": true,
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js"
  ],
  "transform": {
    "\\.tsx?$": "<rootDir>/config/jest/transformers/typescript.js"
  }
}
